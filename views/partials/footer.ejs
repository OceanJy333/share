    </div>

    <!-- 错误提示 -->
    <div id="error-toast" class="toast error-toast">
      <div class="toast-content">
        <i class="fas fa-exclamation-circle toast-icon"></i>
        <span id="error-message" class="toast-message"></span>
      </div>
    </div>
    
    <!-- 成功提示 -->
    <div id="success-toast" class="toast success-toast">
      <div class="toast-content">
        <i class="fas fa-check-circle toast-icon"></i>
        <span id="success-message" class="toast-message"></span>
      </div>
    </div>
  </div>
  
  <script src="https://cdn.jsdelivr.net/npm/particles.js@2.0.0/particles.min.js"></script>
  <!-- 语法高亮 JavaScript - 核心库已包含常用语言支持 -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.7.0/highlight.min.js"></script>
  <script src="/js/particles-config.js?v=<%= Date.now() %>"></script>
  <script src="/js/main.js?v=<%= Date.now() %>"></script>
  <script src="/js/syntax-highlight.js?v=<%= Date.now() %>"></script>
</body>
</html>
